<settings
	xsi:schemaLocation="http://www.tvd12.com/schema/ezyfox/settings
  		ezy-zone-settings-1.0.0.xsd"
	xmlns="http://www.tvd12.com/schema/ezyfox/settings" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<max-users>1000000</max-users>
	
	<user-management>
		<allow-guest-login>false</allow-guest-login>
		<user-max-idle-time>15</user-max-idle-time>
		<max-session-per-user>15</max-session-per-user>
		<user-name-pattern>.*</user-name-pattern>
	</user-management>

	<applications>
		<application>
			<name>notification-message</name>
			<entry-loader>org.youngmonkeys.notification.message.app.entry.NotificationMessageAppEntryLoader</entry-loader>
			<package-name>org.youngmonkeys.notification.message.app</package-name>
		</application>
	</applications>
</settings>
